# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Cache Configuration
CACHE_DISTANCE_THRESHOLD=0.15
CACHE_TTL=604800
CACHE_INDEX_NAME=semantic_cache

# Embedding Model (Multilingual - supports Indonesian/English)
EMBEDDING_MODEL=paraphrase-multilingual-MiniLM-L12-v2

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# Optional: OpenAI API for LLM integration
# OPENAI_API_KEY=your_key_here
